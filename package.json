{
  "scripts": {
    "start:test1": "npm run asbuild:optimized && node ./test1.js",
    "start:hello-world": "npm run asbuild:optimized && node ./hello-world.js",
    "start:exports": "npm run asbuild:exports && node ./exports.js",
    "asbuild:optimized": "asc assembly/index.ts -b build/optimized.wasm -t build/optimized.wat --sourceMap --validate --optimize",
    "asbuild": "npm run asbuild:optimized",
    "asbuild:exports": "asc assembly/exports.ts -b build/exports.wasm -t build/exports.wat --sourceMap --validate --optimize"
  },
  "devDependencies": {
    "assemblyscript": "^0.8.1"
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.8.1"
  }
}
